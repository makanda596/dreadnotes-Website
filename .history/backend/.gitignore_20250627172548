# Ignore environment variables
.env

# Ignore system files
.DS_Store
Thumbs.db

# Ignore log files
*.log
npm-debug.log*

# Ignore node modules
node_modules/

# Ignore build folders
dist/
build/

# Ignore history or backup folders (custom)
history/
backup/
*.bak

# Ignore IDE folders
.vscode/
.idea/

# Optional: ignore uploads
uploads/
